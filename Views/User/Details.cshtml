@model UserDTO

<table class="table">
    <thead>
        <tr>
            <th scope="col">Email</th>
            <th scope="col">First name</th>
            <th scope="col">Last name</th>
            <th scope="col">City</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.Email</th>
            <td>@Model.FirstName</td>
            <td>@Model.LastName</td>
            <td>@Model.City</td>
        </tr>
    </tbody>
</table>