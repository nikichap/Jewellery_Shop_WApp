@model OrderDTO

<h1>Delete Order</h1>

<form asp-action="DeleteConfirm" asp-route-id="@Model.Id">
    <div class="form-group">
        <label>Price</label>
        <input type="number" class="form-control" value="@Model.Item" disabled>
    </div>
    <div class="form-group">
        <label>Stock</label>
        <input type="number" class="form-control" value="@Model.Quantity" disabled>
    </div>
    <label>Stock</label>
    <input type="number" class="form-control" value="@Model.CreatedBy" disabled>
    </div>
    <div class="form-group">
        <label>Stock</label>
        <input type="number" class="form-control" value="@Model.OrderedOn" disabled>
    </div>
    <div class="form-group">
        <label>Stock</label>
        <input type="number" class="form-control" value="@Model.FullPrice" disabled>
    </div>
    <button type="submit" class="btn btn-primary">Delete</button>
    <a asp-action="Index">
        <button type="button" class="btn btn-secondary">Cancel</button>
    </a>
</form>
